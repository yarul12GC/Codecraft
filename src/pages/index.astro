---
import Layout from "../layouts/Layout.astro";
import Card from "../components/Card.astro";
import Header from "../components/header.astro";
import Info from "../components/info.astro";
import Contact from "../components/contact.astro";

const contactFields = [
  { label: "Nombre de la empresa o cliente", name: "nombre" },
  { label: "Correo electrónico", name: "email" },
  { label: "Teléfono", name: "telefono" },
  { label: "Tipo de servicio", name: "servicio" },
  { label: "Descripción breve del proyecto", name: "descripcion" },
  { label: "URL del sitio web (si aplica)", name: "url" },
  { label: "Plazo de entrega estimado", name: "plazo" },
];
---

<Layout title="Welcome to Astro." />
<Header logo="CODECRAFT" title="Codecraft" />
<Info title="Hola," />
<Contact listfor={contactFields} />
